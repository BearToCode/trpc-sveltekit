{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": { "cache": false },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".svelte-kit/**"]
    },
    "preview": { "dependsOn": ["^build"] },
    "lint": { "outputs": [] },
    "format": { "outputs": [] }
  }
}
